{
    "template": "news_feed_macros",
    "description": "Add latest activity live updates",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "/<xf:macro name=\"feed_row\" arg-item=\"!\">.*<li/sU",
    "replace": "<xf:macro name=\"feed_row\" arg-item=\"!\">\n\t<li data-thlclu-id=\"{$item.news_feed_id}\""
}